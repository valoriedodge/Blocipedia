.center
 %h1 All Wikis
.center
 %table
  - @wikis.each do |wiki|
   %tr
    %td= link_to markdown(wiki.title), wiki
    %td= link_to "edit", edit_wiki_path(wiki)
    %td= link_to "delete", [wiki], method: :delete, data: {confirm: 'Are you sure you want to delete this wiki?'}
.center
 = link_to "New Wiki", new_wiki_path, class: 'btn btn-success'
